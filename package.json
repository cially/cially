    "@commitlint/config-conventional": "catalog:",
    "lefthook": "catalog:",
    "taze": "catalog:"
  },
  "postinstall": "lefthook install",
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.6",
      "@types/react-dom": "19.1.6"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "lefthook",
      "sharp"
