  "scripts": {
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "docker:bot": "docker --target cially-bot -t cially-bot .",
    "docker:web": "docker --target cially-web -t cially-web .",
    "up": "taze -r -I",
